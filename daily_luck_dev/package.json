    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "moment": "^2.19.1",
    "moment-range": "^3.0.3",
    "mongoose": "^4.12.3",
    "node-sass-chokidar": "0.0.3",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "numeral": "^2.0.6",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "rc-slider": "^8.3.3",
    "react": "^16.0.0",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.0.0",
    "react-onclickoutside": "^6.6.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.3.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
